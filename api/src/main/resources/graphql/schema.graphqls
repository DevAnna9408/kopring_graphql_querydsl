schema {
    query: Query,
    mutation: Mutation
}

type User {
    oid: ID!,
    userId: String!,
    name: String!,
    email: String!,
    role: String!,
    status: String!
}

input SignUpIn {
    userId: String!,
    name: String!,
    email: String!,
    password: String!
}

type Query {
    findAllUsers: [User],
    findByUserId(userId: String!): User
}

type Mutation {
    createUser(signUpIn: SignUpIn): User,
    deleteUserByUserOid(userOid: ID!): Boolean
}
