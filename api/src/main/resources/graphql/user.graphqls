schema {
    query: Query,
    mutation: Mutation
}

type UserOut {
    oid: ID!,
    userId: String!,
    name: String!,
    email: String!
}

input SignUpIn {
    userId: String!,
    name: String!,
    email: String!,
    password: String!
}

type Query {
    findAllUsers: [UserOut],
    findByUserId(userId: String!): UserOut
}

type Mutation {
    createUser(signUpIn: SignUpIn): UserOut,
    deleteUserByUserOid(userOid: ID!): Boolean
}
